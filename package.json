{
	"name": "vite-express-nodejs-typescript-backend",
	"description": "boilerplate",
	"version": "0.0.0",
	"private": true,
	"license": "UNLICENSED",
	"author": "Sean",
	"type": "module",
	"packageManager": "yarn@4.1.0",
	"engines": {
		"node": ">=18.19.0",
		"yarn": ">=4.1.0"
	},
	"scripts": {
		"start": "yarn install && nodemon --watch src --ext .ts,.js --exec tsx ./src/index.ts",
		"build": "cross-env CI=false  NODE_ENV=local node esbuild.config.js",
		"build:dev": "cross-env CI=false  NODE_ENV=development node esbuild.config.js",
		"build:prod": "cross-env CI=false NODE_ENV=production node esbuild.config.js",
		"preview": "node dist/index.cjs",
		"lint:check": "eslint src/**/*.ts",
		"lint:fix": "eslint --fix src/**/*.ts"
	},
	"dependencies": {
		"compression": "^1.7.4",
		"cookie-parser": "^1.4.6",
		"cors": "^2.8.5",
		"dotenv": "^16.4.3",
		"express": "^4.18.2",
		"lodash": "^4.17.21",
		"mongoose": "^8.1.2",
		"morgan": "^1.10.0",
		"reflect-metadata": "^0.2.1",
		"typedi": "^0.10.0"
	},
	"devDependencies": {
		"@dotenv-run/esbuild": "^1.3.4",
		"@types/compression": "^1.7.5",
		"@types/cookie-parser": "^1.4.6",
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/lodash": "^4.14.202",
		"@types/mongoose": "^5.11.97",
		"@types/morgan": "^1.9.9",
		"@types/node": "^20.11.17",
		"@typescript-eslint/eslint-plugin": "^7.0.1",
		"@typescript-eslint/parser": "^7.0.1",
		"corepack": "^0.24.1",
		"cross-env": "^7.0.3",
		"esbuild": "^0.20.0",
		"esbuild-plugin-clean": "^1.0.1",
		"eslint": "^8.56.0",
		"eslint-plugin-node": "^11.1.0",
		"nodemon": "^3.0.3",
		"tsx": "^4.7.1",
		"typescript": "^5.3.3"
	}
}
